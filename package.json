{
  "dependencies": {
    "probot-serverless-now": "^0.2.1",
    "debug": "^4.1.0",
    "github-backport": "^0.2.0",
    "shared-github-internals": "^0.2.0"
  },
  "description": "GitHub App to backport pull requests",
  "devDependencies": {
    "@octokit/rest": "^15.18.0",
    "@types/debug": "^0.0.31",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "probot": "^7.4.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
  "main": "app",
  "name": "backport",
  "private": true,
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,md,ts,yml}\"",
    "prestart": "yarn build",
    "start": "probot run .",
    "test": "jest",
    "tslint": "tslint --format stylish --project ."
  }
}
